<%= form_for(@group) do |f| %>
	<%= f.label :users %>
	<% if false %><%= f.text_field :users, :value => "" %><% end %>
	<%= f.autocomplete_field :users, autocomplete_user_name_groups_path, :value => "" %>

	<%= f.label(:book, "Enter ISBN")%>
	<%= f.autocomplete_field :book, autocomplete_book_title_groups_path, :value => "" %>
	

	<%= f.submit "Submit" %>
<% end %> 